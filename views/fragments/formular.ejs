<form id="form_inreg"  method="post" class="date"  action="<%= locals.action %>"  enctype="multipart/form-data" >
    <p>
        <label>
            Username: <input type="text" name="username" id="inp-username" value="prof<%= Math.floor(Math.random()*100000) %>" required pattern="[A-Za-z0-9#_./]+">
        </label>
    </p>
    <p>
        <label>
            Last Name: <input type="text" name="nume" id="inp-nume"  value="Gogulescu" required>
        </label>
    </p>
    <p>
        <label>
            First Name: <input type="text" name="prenume" id="inp-prenume" value="Gogu"  >
        </label>
    </p>
    <p>
        <label>
            Password: <input id="parl" type="password" required  name="parola" value="">
        </label>
    </p>
    <p id="p-reintrod">
        <label >
            Retype password: <input id="rparl" required type="password" name="rparola" value="">
        </label>
    </p>
    <p>
        <label>
            Email: <input type="text" name="email" id="inp-email" required value="profprofprof007@gmail.com"  size="50">
        </label>
    </p>
    <p>
        <label>
            Phone Number: <input type="tel" name="tel" id="inp-tel" required value=""  size="50">
        </label>
    </p>
    <p>
        <label>
            Birth date: <input type="date" name="tel" id="inp-bdate" required value="">
        </label>
    </p>
    <p>
        <label>
            Chat text color: <select name="culoare_chat" id="sel-culoare"><option value="red" selected>rosu</option><option value="green">verde</option><option value="blue">albastru</option></select>
        </label>
    </p>
    <p>
        <label>
            Profile picture: <input type="file" name="poza">
        </label>			
    </p>
    <p>		
        <input type="submit" value="Trimite">
        <input type="reset" value="Reseteaza">
    </p>
</form>